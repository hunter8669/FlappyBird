language = "python3"
run = "python main.py"

[packager]
language = "python3"
ignoredPackages = ["unit_tests"]

[packager.features]
enabledForHosting = false
packageSearch = true
guessImports = true

modules = ["python-3.11"]

[deployment]
run = ["sh", "-c", "python main.py"]

[nix]
channel = "stable-22_11"

[[ports]]
localPort = 8000
externalPort = 80
